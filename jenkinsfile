    }
    stage('Test') { 
        echo " I'm in Test Stage" 
    }
    stage('Deploy') { 
        echo " I'm in Deploy Stage" 
    }
    stage('Archive') { 
        echo " I'm in Archive Stage" 
    }
    stage('Cleanup') { 
        echo " I'm in Cleanup Stage" 
    }    
}
